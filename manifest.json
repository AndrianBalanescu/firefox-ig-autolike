{
  "applications": {
    "gecko": {
      "id": "igautolike@vinsce.me",
      "strict_min_version": "42.0",
      "strict_max_version": "*"
    }
  },
  "manifest_version": 2,
  "name": "IG Autolike",
  "version": "0.1.6",
  "description": "Instagram auto like extension for Firefox.",
  "icons": {
    "48": "icons/ig_logo.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "autolike.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "icons/ig_logo.png"
    }
  },
  "options_ui": {
    "page": "html/options.html"
  }
}
